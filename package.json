    "build:production": "cross-env NODE_ENV=production next build",
    "clean": "rm -rf .next node_modules/.cache",
    "dev": "next dev",
    "full-build": "node scripts/build-and-analyze.js",
    "lint": "next lint --fix",
    "performance": "node scripts/build-performance.js",
    "pre-build": "node scripts/pre-build-check.js",
    "quick-build": "node scripts/quick-build.js",
    "start": "next start",
    "type-check": "tsc --noEmit || echo 'Type check skipped'"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slider": "latest",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "child_process": "latest",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fs": "latest",
    "lucide-react": "^0.454.0",
    "next": "14.2.16",
    "path": "latest",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
